{
  "expo": {
    "name": "Wiskipix App",
    "slug": "wiskipix-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF0132"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Las fotos en papel son un recuerdo para toda la vida. Te ayudamos a conservar esos momentos imprimiendo tus fotos.",
    "githubUrl": "https://github.com/cesarmejiag/wp-app"
  }
}
